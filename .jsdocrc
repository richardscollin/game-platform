{
  "opts": {
    "access": "all",
    "destination": "./docs/jsdoc/",
    "readme": "README.md",
    "recurse": true,
    "pedantic": true,
    "tutorials": "docs/tutorials"
  },
  "plugins": [
    "plugins/external-links"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.m?js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
